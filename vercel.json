{
    "rewrites": [
      { "source": "/(.*).js", "destination": "/$1.js" },
      { "source": "/(.*).css", "destination": "/$1.css" },
      { "source": "/(.*).png", "destination": "/$1.png" },
      { "source": "/(.*).jpg", "destination": "/$1.jpg" },
      { "source": "/(.*).svg", "destination": "/$1.svg" },
      { "source": "/(.*).ico", "destination": "/$1.ico" },
      { "source": "/(.*).json", "destination": "/$1.json" },
      { "source": "/(.*).woff", "destination": "/$1.woff" },
      { "source": "/(.*).woff2", "destination": "/$1.woff2" },
      { "source": "/(.*).ttf", "destination": "/$1.ttf" },
      { "source": "/(.*).eot", "destination": "/$1.eot" },
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/(.*).js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }