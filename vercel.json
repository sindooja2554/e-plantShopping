{
    "rewrites": [
      {
        "source": "/((?!.*\\.(js|css|png|jpg|svg|ico|json|woff|woff2|ttf|eot)).*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*).js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
}